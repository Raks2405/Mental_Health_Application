{
  "cli": {
    "version": ">= 16.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    
    
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "buildConfiguration": "Release"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "image": "latest",
        "buildConfiguration": "Release"
      },
      "env": {"RCT_NEW_ARCH_ENABLED": "1"}
    }
  },
  "submit": {
    "production": {}
  }
}
